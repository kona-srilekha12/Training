"""
# set bits
s=int(input())
if s>0:
    s=bin(s)
    s=s[2:]
    count=0
    print(s)
    for i in range(len(s)):
        if s[i]=="1":
            count+=1
    print(count)
else :
    s=bin(s)
    s=s[3:]
    count=0
    print(s)
    for i in range(len(s)):
        if s[i]=="1":
            count+=1
    print(count)
