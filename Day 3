"""
# power of 2
n=int(input("enter a number"))
c=0
if  n==1 :
    print("it is 2 power number")
else :
    for i in range(n//2):
        if 2**i==n  :
            print("it is 2 power number")
            c+=1
            break
    if c==0:
        print("it is not a 2 power number")
n=int(input("enter a number"))
if  n==1 :
    print("it is 2 power number")
else :
    for i in range(n//2):
        if 4**i==n  :
            print("it is 4 power number")
            c+=1
            break
    if c==0:
        print("it is not a 4 power number")





    
